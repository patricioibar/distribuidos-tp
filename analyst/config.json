    "batch-size": 100,
    "coffee-analyzer-address": "localhost:30001",
    "log-level": "info",
    "tables": [
        {
            "name": "menu_items",
            "columns": ["item_id", "item_name"]
        },
        {
            "name": "stores",
            "columns": ["store_id", "store_name"]
        },
        {
